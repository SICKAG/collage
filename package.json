{
  "name": "@sick-davinci/collage",
  "version": "0.0.0",
  "description": "A library to handle communication between micro frontends and enable an easy to use orchestration",
  "repository": "https://gitlab.sick.com/davinci/libraries/collage.git",
  "author": "Wanja Tschuor <wanja.tschuor@sick>",
  "contributors": [
    "Susanne Kirchner <susanne.kirchner@sick.de>",
    "Joshua Hagedorn <ravens.aerie@googlemail.com"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/collage.umd.js",
  "module": "./dist/collage.es.js",
  "typings": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/collage.es.js",
      "require": "./dist/collage.umd.js"
    }
  },
  "scripts": {
    "start:dev": "vite",
    "start:doc": "cross-env DEVELOP=true vuepress-vite dev ./docs -c ./docs/vuepress-config.js -p 8090",
    "start": "run-p start:*",
    "build": "tsc && vite build",
    "build:doc": "vuepress-vite build ./docs -c ./docs/vuepress-config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "lint": "eslint ./src --ext js,ts",
    "lint:fix": "eslint ./src --ext js,ts --fix",
    "wdio": "wdio run e2e/wdio.conf.js",
    "test:wdio": "run-p start:dev wdio --race"
  },
  "keywords": [],
  "dependencies": {
    "lodash.mergewith": "^4.6.2",
    "penpal": "^6.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/lodash.mergewith": "^4.6.6",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vuepress/theme-default": "^2.0.0-beta.52",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "jest-junit": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vite": "^3.1.8",
    "vite-plugin-dts": "^1.6.6",
    "vuepress-vite": "^2.0.0-beta.52"
  }
}
